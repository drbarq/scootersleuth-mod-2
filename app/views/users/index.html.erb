<div class="container large">
  <h1 class="text-center">Welcome to Scooter Sleuth!</h1>

  <article class="content"
    <ul>
      <% @users.each do |user| %>
      <li><%= link_to user.full_name, user_path(user.id) %></li>
      <% end %>
    </ul>

    <a href="users/new" id="new_user"><button class='btn btn-primary btn-lg'>Create New User</button></a>

  </article>
</div>
