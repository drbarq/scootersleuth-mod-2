<div class="container">
  <h1>Welcome to Scooter Sleuth!</h1>

  <article class="content"
    <ul>
      <% @users.each do |user| %>
      <li><%= link_to user.full_name, user_path(user.id) %></li>
      <% end %>
    </ul>

    <h4 id="new_user">
      <span><%= link_to 'Create New User', new_user_path %><span>
    </h4>
  </article>
</div>
