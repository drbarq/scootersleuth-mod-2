<h1>Enter Your Current Location:</h1>
<br>
  <%= form_for :user, url: users_path do |f| %>
    <label>Bird<%= f.check_box :bird %></label>
    <label>Lime<%= f.check_box :lime %></label>
    <label>Spin<%= f.check_box :spin %></label>
<% end %>

  <%= form_for :location, url: locations_path do |f| %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.submit %>
  <% end %>
